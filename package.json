{
  "name": "user-mng-app",
  "version": "0.1.0",
  "private": true,
  "main": "public/Main.js",
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "antd": "^5.23.1",
    "axios": "^1.7.9",
    "child_process": "^1.0.2",
    "concurrently": "^9.1.2",
    "cra-template": "1.2.0",
    "cross-env": "^7.0.3",
    "fs": "^0.0.1-security",
    "http-proxy-middleware": "^3.0.3",
    "protobufjs": "^7.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "react-scripts": "5.0.1",
    "wait-on": "^8.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "wait-on http://localhost:3000 && set ELECTRON_START_URL=http://localhost:3000 && electron .", 
    "electron-pack": "npm run build && electron-builder build --win portable -c.extraMetadata.main=build/Main.js",
    "react-start": "set BROWSER=none && npm start",
    "all-start": "concurrently \"npm run react-start\" \"npm run electron-start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1"
  }
}
